{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ],
        "urls": [
            "https://fonts.googleapis.com/css?family=Material+Icons",
            "https://fonts.gstatic.com/s/materialicons/**"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [{
    "name": "fresh",
    "urls": ["/api/v2/tag/feedIDs", "/api/v2/feed", "/api/v2/article"],
    "cacheConfig": {
      "strategy": "freshness",
      "timeout": "2s",
      "maxAge": "3d",
      "maxSize": 100
    }
  }, {
    "name": "perf",
    "urls": ["/api/v2/features"],
    "cacheConfig": {
      "strategy": "performance",
      "timeout": "2s",
      "maxAge": "3d",
      "maxSize": 100
    }
  }]
}
